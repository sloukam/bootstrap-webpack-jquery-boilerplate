{
  "name": "bootstrap-webpack-jquery-boilerplate",
  "license": "MIT",
  "author": "xxhomey19",
  "homepage": "https://xxhomey19.github.io/bootstrap-webpack-jquery-boilerplate/",
  "repository": {
    "type": "git",
    "url": "https://github.com/xxhomey19/bootstrap-webpack-jquery-boilerplate/tree/master"
  },
  "bugs": {
    "url": "https://github.com/xxhomey19/bootstrap-webpack-jquery-boilerplate/issues"
  },
  "version": "1.6.4",
  "scripts": {
    "build": "webpack -p --progress --mode production --config webpack.config.js",
    "build-postgre": "npm-run-all clean lint-postgre tsc copy-assets parcel",
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --open --config webpack.config.js",
    "dev-postgre": "nodemon --watch src -e ts,ejs --exec npm run dev:start-postgre",
    "dev:start-postgre": "npm-run-all build-postgre start-postgre",
    "initdb": "ts-node tools/initdb",
    "lint": "eslint src/js",
    "lint-postgre": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint:fix": "npm run lint -- --fix",
    "parcel": "parcel build src/public/js/*.ts -d dist/public/js",
    "start": "npm run dev",
    "start-postgre": "node .",
    "test": "jest --coverage",
    "tsc": "tsc"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "materialize-css": "^1.0.0",
    "pg": "^7.14.0",
    "pg-promise": "^10.3.1",
    "popper.js": "^1.15.0",
    "summernote": "^0.8.12",
    "trumbowyg": "^2.19.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.15.16",
    "@types/fs-extra": "^8.0.1",
    "@types/materialize-css": "^1.0.7",
    "@types/pg": "^7.11.2",
    "@types/shelljs": "^0.8.6",
    "@types/vue": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "css-url-relative-plugin": "^1.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jquery": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^2.7.0",
    "image-webpack-loader": "^5.0.0",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.7.0",
    "mustache": "^3.0.1",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "parcel-bundler": "^1.12.4",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "^1.18.2",
    "prettier-package-json": "^2.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "terser-webpack-plugin": "^1.3.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
